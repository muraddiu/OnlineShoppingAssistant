<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Manage</servlet-name>
        <servlet-class>com.osa.servlet.Manage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegistration</servlet-name>
        <servlet-class>com.osa.servlet.UserRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateLogIn</servlet-name>
        <servlet-class>com.osa.servlet.ValidateLogIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.osa.servlet.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartHandler</servlet-name>
        <servlet-class>com.osa.servlet.CartHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DBDelete</servlet-name>
        <servlet-class>com.osa.servlet.DBDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartPurces</servlet-name>
        <servlet-class>com.osa.servlet.CartPurces</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>com.osa.servlet.DownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>underprocess</servlet-name>
        <servlet-class>com.osa.servlet.underprocess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Successfull</servlet-name>
        <servlet-class>com.osa.servlet.Successfull</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Manage</servlet-name>
        <url-pattern>/Manage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegistration</servlet-name>
        <url-pattern>/UserRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateLogIn</servlet-name>
        <url-pattern>/ValidateLogIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartHandler</servlet-name>
        <url-pattern>/CartHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DBDelete</servlet-name>
        <url-pattern>/DBDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartPurces</servlet-name>
        <url-pattern>/CartPurces</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/DownloadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>underprocess</servlet-name>
        <url-pattern>/underprocess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Successfull</servlet-name>
        <url-pattern>/Successfull</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
